!x -> mod1
!keycode x = Alt_L
!
!x -> mod3
!remove mod4 = Hyper_L
!add mod3 = Hyper_L
!keycode x = Hyper_L
!
!x -> mod5
!keycode x = ISO_Level3_Shift
!
!x -> Mode_switch
!keycode x = Mode_switch


remove mod4 = Hyper_L
add mod3 = Hyper_L
keycode 59 = Hyper_L
keycode 60 = ISO_Level3_Shift
keycode 61 = Mode_switch

