#!/bin/bash

. $ul_bindir/ul/init-ul --only-vars

yes | rm -r $ul_tmpdir/git &>/dev/null
git clone "https://github.com/memoryLeak47/utils-library.git" $ul_tmpdir/git

if [ "$?" ];
then
	rm -r $ul_bindir
	cp -r $ul_tmpdir/git/bin $ul_bindir
	rm -r $ul_resdir
	cp -r $ul_tmpdir/git/res $ul_resdir
	yes | rm -r $ul_tmpdir/git
fi
