#!/bin/bash
# cdb v0

req "readlink"

if [ "$1" -eq "$1" ] 2>/dev/null; then
	for (( c=0; c<$1; c++ )); do
		cd ..
	done
else
	DIR=`readlink -f .`
	while [ ! a$1 == a${x##*\/} ]
	do
		cd ..
		x=`readlink -f .`
		if [ a$x == a'/' ]; then
			cd $DIR
			echo "Directory '$1' not found."
			exit
		fi
	done
fi
