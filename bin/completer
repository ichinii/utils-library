#!/bin/bash

usage="Usage:\tcompleter -a\n\tcompleter <binfile>"

if [ -z "$1" ]; then
	echo -e "$usage"
	exit
fi

if [ "$1" == "sc" ] || [ "$1" == "-a" ]; then
	complete -W "j jc jr c cc cr cpp cppc cppr" sc
fi

if [ "$1" == "cb" ] || [ "$1" == "-a" ]; then
	complete -W "$(cd $ul_tmpdir/cb; find -wholename "./*" | sed 's/^.\///g')" cb 
fi

if [ "$1" == "goto" ] || [ "$1" == "-a" ]; then
	tmp=$(cat $ul_tmpdir/gotolib)
	res=
	for ok in $tmp
	do
		res="$res ${ok%:*}"
	done
	complete -W "$res" goto
fi
